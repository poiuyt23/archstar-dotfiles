# Examples of commands:
"xbindkeys_show" 
  control+shift + q

"systemctl suspend"
   Control+Alt + s
"xscreensaver-command -lock"
   Control+Alt + l
"$HOME/scripts/show_keybinds.sh"
   Alt+Mod4 + k
"kitty"
   Alt+Mod4 + t
"firefox"
   Alt+Mod4 + f
"brave"
   Alt+Mod4 + b 
"sudo /home/poiuyt/scripts/brightness_up.sh"
   Mod4+Alt + F6
"sudo /home/poiuyt/scripts/brightness_down.sh"
   Mod4+Alt + F5
"$HOME/scripts/toggle_touchpad.sh"
   Mod4+Alt + F9
"urxvt -e $HOME/scripts/light_up.sh"
   Mod4+Alt + F4
"urxvt -e $HOME/scripts/light_down.sh"
   Mod4+Alt + F3
"$HOME/scripts/toggle_touchpad.sh"
   Mod4+Alt + F9
